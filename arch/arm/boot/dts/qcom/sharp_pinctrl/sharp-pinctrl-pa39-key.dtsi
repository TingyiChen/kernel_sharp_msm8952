&soc {
	tlmm_pinmux: pinctrl@1000000 {
		compatible = "qcom,msm-tlmm-8916";
		reg = <0x1000000 0x300000>;
		interrupts = <0 208 0>;

		tlmm_gpio_key {
			qcom,pins = <&gp 85>, <&gp 131>, <&gp 132>;
			qcom,pin-func = <0>;
			qcom,num-grp-pins = <3>;
		};
	};
};
