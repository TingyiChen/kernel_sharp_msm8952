/*****************************************/
/*  pm8952: qcom,pm8952 alias            */
/*****************************************/

&spmi_bus {
	
	qcom,pm8950@0 {
		qcom,power-on@800 {
			qcom,s3-debounce = <16>;
			qcom,s3-src = "kpdpwr";

			qcom,pon_1 {
				qcom,pon-type = <0>;
				qcom,pull-up = <1>;
				linux,code = <116>;
				qcom,support-reset = <1>;
				qcom,s1-timer = <6720>;
				qcom,s2-timer = <1000>;
				qcom,s2-type = <7>;
			};

			qcom,pon_2 {
				qcom,pon-type = <1>;
//				qcom,support-reset = <1>;
				qcom,support-reset = <0>;
				qcom,pull-up = <1>;
//				qcom,s1-timer = <6720>;
//				qcom,s2-timer = <1000>;
//				qcom,s2-type = <7>;
//				linux,code = <114>;
				linux,code = <115>;
//				qcom,use-bark;
			};

			qcom,pon_3 {
				qcom,pon-type = <3>;
				qcom,support-reset = <1>;
				qcom,s1-timer = <6720>;
				qcom,s2-timer = <1000>;
				qcom,s2-type = <7>;
				qcom,pull-up = <1>;
				qcom,use-bark;
			};
		};
	};

	qcom,pm8950@0 {
	};

	qcom,pm8950@1 {
	};
};

&soc {
	eldo2_8952: eldo2 {
		/delete-property/ gpio;
	};
};
/*****************************************/
/*  pm8950-gpios: qpnp-pin               */
/*****************************************/

&pm8950_gpios {

	gpio@c000 { /* GPIO_01 */
		/* CDC_PM_MCLK */
		qcom,mode = <0>;			/* DIG_IN */
		qcom,output-type = <0>;		/* CMOS */
		qcom,invert = <0>;			/* DISABLE */
		qcom,pull = <4>;			/* PULL_DN */
		qcom,vin-sel = <0>;			/* L5 */
		qcom,out-strength = <1>;	/* LOW */
		qcom,src-sel = <0>;			/* CONSTANT */
		qcom,master-en = <1>;		/* ENABLE */
		status = "okay";
	};

	gpio@c100 { /* GPIO_02 */
		/* SP_CLOCK */
		qcom,mode = <0>;			/* DIG_IN */
		qcom,output-type = <0>;		/* CMOS */
		qcom,invert = <0>;			/* DISABLE */
		qcom,pull = <4>;			/* PULL_DN */
		qcom,vin-sel = <0>;			/* L5 */
		qcom,out-strength = <1>;	/* LOW */
		qcom,src-sel = <0>;			/* CONSTANT */
		qcom,master-en = <1>;		/* ENABLE */
		status = "okay";
	};

	gpio@c200 { /* GPIO_03 */
		/* UIM_BATT_ALARM */
		/* Qualcomm Settings */
	};

	gpio@c300 { /* GPIO_04 */
		/* HS_MIC_SW */
		qcom,mode = <0>;			/* DIG_IN */
		qcom,output-type = <0>;		/* CMOS */
		qcom,invert = <0>;			/* DISABLE */
		qcom,pull = <4>;			/* PULL_DN */
		qcom,vin-sel = <2>;			/* L5 */
		qcom,out-strength = <1>;	/* LOW */
		qcom,src-sel = <0>;			/* CONSTANT */
		qcom,master-en = <1>;		/* ENABLE */
		status = "okay";
	};

	gpio@c400 { /* GPIO_05 */
		/* DIS_SW */
		qcom,mode = <0>;			/* DIG_IN */
		qcom,output-type = <0>;		/* CMOS */
		qcom,invert = <0>;			/* DISABLE */
		qcom,pull = <4>;			/* PULL_DN */
		qcom,vin-sel = <2>;			/* L5 */
		qcom,out-strength = <1>;	/* LOW */
		qcom,src-sel = <0>;			/* CONSTANT */
		qcom,master-en = <1>;		/* ENABLE */
		status = "okay";
	};

	gpio@c500 { /* GPIO_06 */
		/* DIS_SW */
		qcom,mode = <0>;			/* DIG_IN */
		qcom,output-type = <0>;		/* CMOS */
		qcom,invert = <0>;			/* DISABLE */
		qcom,pull = <4>;			/* PULL_DN */
		qcom,vin-sel = <2>;			/* L5 */
		qcom,out-strength = <1>;	/* LOW */
		qcom,src-sel = <0>;			/* CONSTANT */
		qcom,master-en = <1>;		/* ENABLE */
		status = "okay";
	};

	gpio@c600 { /* GPIO_07 */
		/* - */
		qcom,mode = <0>;			/* DIG_IN */
		qcom,output-type = <0>;		/* CMOS */
		qcom,invert = <0>;			/* DISABLE */
		qcom,pull = <4>;			/* PULL_DN */
		qcom,vin-sel = <2>;			/* L5 */
		qcom,out-strength = <1>;	/* LOW */
		qcom,src-sel = <0>;			/* CONSTANT */
		qcom,master-en = <1>;		/* ENABLE */
		status = "okay";
	};

	gpio@c700 { /* GPIO_08 */
		/* AUDIO_OUT_SW */
		qcom,mode = <0>;			/* DIG_IN */
		qcom,output-type = <0>;		/* CMOS */
		qcom,invert = <0>;			/* DISABLE */
		qcom,pull = <4>;			/* PULL_DN */
		qcom,vin-sel = <2>;			/* L5 */
		qcom,out-strength = <1>;	/* LOW */
		qcom,src-sel = <0>;			/* CONSTANT */
		qcom,master-en = <1>;		/* ENABLE */
		status = "okay";
	};

};

/*****************************************/
/*  pm8950-mpps: qpnp-pin                */
/*****************************************/

&pm8950_mpps {

	mpp@a000 { /* MPP_01 */
		/* VDD_PX_BIAS_MPP_1 */
		/* Qualcomm Settings */
	};

	mpp@a100 { /* MPP_02 */
		/* MSM_THERM */
		qcom,mode = <4>;		/* Analog Input */
		qcom,src-sel = <1>;		/* MPP is always enabled */
		qcom,ain-route = <1>;	/* QPNP_PIN_AIN_AMUX_CH6 */
		qcom,master-en = <1>;	/* ENABLE */
		status = "okay";
	};

	mpp@a200 { /* MPP_03 */
		/* VREF_DAC_MPP_3 */
		/* Qualcomm Settings */
	};

	mpp@a300 { /* MPP_04 */
		/* LCD_THERM */
		qcom,mode = <4>;		/* Analog Input */
		qcom,src-sel = <1>;		/* MPP is always enabled */
		qcom,ain-route = <3>;	/* QPNP_PIN_AIN_AMUX_CH8 */
		qcom,master-en = <1>;	/* ENSABLE */
		status = "okay";
	};

};
